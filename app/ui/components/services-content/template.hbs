{{menu-strip items=data selected=_selected onChange=(action 'itemSelected')}}

{{#content-slider
	class='services-content__slider'
	cycle=true
	slideComponentName='service-slide'
	data=data
	selected=_selected
	slideDirection=_slideDirection
	onPrevious=(action 'previousSlide')
	onNext=(action 'nextSlide')
	}}
	{{anfema-logo class="services-content__background"
		step=_logoStep
		options=(hash outlineMode=false strokeWidth=20)}}
{{/content-slider}}
