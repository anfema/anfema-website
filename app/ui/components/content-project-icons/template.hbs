{{yield}}
<h3 class="content-project-icons__title">{{data.title}}</h3>
<div class="content-project-icons__container">
	{{#each data.icons as |icon|}}
		<img class="content-project-icons__icon" src="/img/{{icon}}.svg" alt="{{icon}}">
	{{/each}}
</div>
<p class="content-project-icons__text">{{data.text}}</p>
