{{#page-overlay}}
	{{#page-menu forceOpaque=true class="page-menu--flat"}}
		<header class="breadcrumbs">
			{{#link-to "projects"}}
				<span class="breadcrumbs__crumb">{{t "index.nav.projects"}}</span>
			{{/link-to}}
			<span class="breadcrumbs__separator"> / </span>
			<span class="breadcrumbs__crumb">{{model.meta.client}} {{model.meta.title}}</span>
		</header>
	{{/page-menu}}

	{{#project-detail data=model}}
		{{#each model.sections as |data|}}
	<div class="test">
		<svg width="100" height="100" viewBox="0 0 24 24">
			<path d="M21,9H15V22H13V16H11V22H9V9H3V7H21M12,2A2,2 0 0,1 14,4A2,2 0 0,1 12,6C10.89,6 10,5.1 10,4C10,2.89 10.89,2 12,2Z" />
		</svg>
	</div>
			{{#if data.features}}
				{{component data.component data=data selected=topfeatures}}
			{{else}}
				{{component data.component data=data meta=model.meta}}
			{{/if}}
		{{/each}}
	{{/project-detail}}
{{/page-overlay}}

{{outlet}}